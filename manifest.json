{
  "name": "MobliTab",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "New Tab Page that Suits my workflow",
  "homepage_url": "https://moblimic.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "clipboardRead",
    "clipboardWrite",
    "tts",
    "history",
    "notifications",
    "tabs",
    "topSites",
    "https://*/*"
  ],
  "omnibox": {
    "keyword": "extensionizr"
  }
}
