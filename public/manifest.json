{
  "name": "MobliTab",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "New Tab Page that Suits my workflow",
  "homepage_url": "https://moblimic.com",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "clipboardRead",
    "clipboardWrite",
    "tts",
    "history",
    "notifications",
    "tabs",
    "topSites",
    "https://*/*"
  ],
  "omnibox": {
    "keyword": "moblitab"
  },
  "content_security_policy": "script-src 'self' 'sha256-KC4T4MRWNYtT5sIdN0musuo0QIBmke12fkjB0bh/kZE='; object-src 'self'"
}
